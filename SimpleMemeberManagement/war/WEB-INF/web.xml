<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>SimpleMemberManagement</servlet-name>
		<servlet-class>kr.ac.shinan.csp.SimpleMemberManagementServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SimpleMemberManagement</servlet-name>
		<url-pattern>/simplemembermanagement</url-pattern>
	</servlet-mapping>
	
		
	<servlet>
		<servlet-name>RetrieveTeamMember</servlet-name>
		<servlet-class>kr.ac.shinan.csp.RetrieveTeamMemberServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>RetrieveTeamMember</servlet-name>
		<url-pattern>/RetrieveTeamMember</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>ReadMemberServlet</servlet-name>
		<servlet-class>kr.ac.shinan.csp.ReadMemberServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ReadMemberServlet</servlet-name>
		<url-pattern>/ReadMemberServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UpdateResultServlet</servlet-name>
		<servlet-class>kr.ac.shinan.csp.UpdateResultServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>UpdateResultServlet</servlet-name>
		<url-pattern>/UpdateResultServlet</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteResultServlet</servlet-name>
		<servlet-class>kr.ac.shinan.csp.DeleteResultServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>DeleteResultServlet</servlet-name>
		<url-pattern>/DeleteResultServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SignUp</servlet-name>
		<servlet-class>kr.ac.shinan.csp.SignUpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SignUp</servlet-name>
		<url-pattern>/signup</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Login</servlet-name>
		<servlet-class>kr.ac.shinan.csp.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
		
	</servlet-mapping>
		<servlet>
		<servlet-name>Idcheck</servlet-name>
		<servlet-class>kr.ac.shinan.csp.IDCheckServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Idcheck</servlet-name>
		<url-pattern>/idcheck</url-pattern>
	</servlet-mapping>
	
		<servlet>
		<servlet-name>Entry</servlet-name>
		<servlet-class>kr.ac.shinan.csp.EntryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Entry</servlet-name>
		<url-pattern>/entry</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>login.html</welcome-file>
	</welcome-file-list>
</web-app>
